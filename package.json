{
  "name": "jsdoc-to-condition",
  "description": "Creates validation code from jsdoc comments",
  "version": "0.1.0",
  "main": "lib/index.js",
  "author": "jakwuh <jakwuh@yandex-team.ru>",
  "license": "MIT",
  "repository": "git@github.com:jakwuh/jsdoc-to-condition.git",
  "scripts": {
    "test": "npm run build && ava",
    "build": "mkdir -p lib && babel src -d lib",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "doctrine": "^2.0.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0"
  },
  "ava": {
    "files": [
      "./tests/index.js"
    ],
    "source": [
      "./src/*.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
